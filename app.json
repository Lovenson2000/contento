{
  "expo": {
    "name": "contento",
    "slug": "contento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lovenson.contento",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "myapp",
              "exp+contento",
              "com.lovenson.contento",
              "com.googleusercontent.apps.543473244209-bo2nekaer9ksq4lpkhs8i1om15f7fftj"
            ]
          }
        ]
      },
      "icon": {
        "light": "./assets/images/icons/ios.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lovenson.contento"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.543473244209-bo2nekaer9ksq4lpkhs8i1om15f7fftj"
        }
      ],
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsText": true,
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
            "NSExtensionActivationSupportsWebPageWithMaxCount": 1,
            "NSExtensionActivationSupportsImageWithMaxCount": 2
          },
          "androidIntentFilters": ["text/*", "image/*"]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.jpg",
          "color": "#ffffff",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7dda0cc0-6209-4911-bb1b-5ded06254925"
      }
    }
  }
}
